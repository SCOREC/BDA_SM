    "hyper_params": {
        "epochs": 200,
        "batch_size": 64,
        "data_shape": [4],
        "optimizer": "adam",
        "learning_rate": 0.001,
        "loss_function": "categorical_crossentropy",
        "train_percent": 0.8
    },
    "data": {
        "data_type": "local",
        "data_location": "test_data/data.csv"
    }
}