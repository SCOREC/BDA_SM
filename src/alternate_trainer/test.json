{
    "model_name": "model",
    "version": "1.0",
    "topology": [
        {
            "type": "dense",
            "activation": "relu",
            "units": 64
        }, {
            "type": "dropout",
            "rate": 0.95
        },{
            "type": "dense",
            "activation": "relu",
            "units": 32
        }, {
            "type": "dropout",
            "rate": 0.95
        }, {
            "type": "dense",
            "activation": "softmax",
            "units": 3
        }
